<h1>Listing releases</h1>

<table>
  <tr>
    <th>Project</th>
    <th>Drupal version</th>
    <th>Release version</th>
    <th>Release quality</th>
    <th>Status</th>
  </tr>

<% for release in @releases %>
  <tr>
    <td><%=h release.project.code %></td>
    <td><%=h release.drupal_version %></td>
    <td><%=h release.release_version %></td>
    <td><%=h release.release_status %></td>
    <td><%=h release.status %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New release', new_release_path %>
